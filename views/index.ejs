<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>My Blog</title>
</head>

<body>
    <%- include("partials/header.ejs") %>
        <div class="container">
            <div class="row mt-n5">
             <% try{%>
                <%Object.values(jsonData).forEach(value=> {%>
                    <% Object.keys(value).forEach(key=>{%>
                        <%- include("partials/summary.ejs", {blogTitle: value[key]['blogTitle'], blogDescription: value[key]['blogDescription'], blogContent: value[key]['blogContent'], blogImage: `/data/uploads/${key}.png`, blogId: key })%>
                  <% }) %>
                
            <% })}catch{%>
                <%- include("partials/addNew.ejs")  %>
           <% } %>  

		<div class="row mt-6 wow fadeInUp" data-wow-delay=".6s"
            style="visibility: visible; animation-delay: 0.6s; animation-name: fadeInUp;">
            <div class="col-12">
                <div class="pagination text-small text-uppercase text-extra-dark-gray">
                    <ul>
                        <li><a href="#!"><i class="fas fa-long-arrow-alt-left me-1 d-none d-sm-inline-block"></i>
                                Prev</a></li>
                        <li class="active"><a href="#!">1</a></li>
                        <li><a href="#!">2</a></li>
                        <li><a href="#!">3</a></li>
                        <li><a href="#!">Next <i
                                    class="fas fa-long-arrow-alt-right ms-1 d-none d-sm-inline-block"></i></a></li>
                    </ul>
                </div>
            </div>
        </div> 
            </div>
        </div>
</body>
<%- include("partials/footer.ejs") %>
</html>